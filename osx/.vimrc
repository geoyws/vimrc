set nocompatible
syntax on
"colo jellybean
"set clipboard=unnamedplus
set list listchars=tab:\|\ 
hi SpecialKey ctermfg=255 guifg=#eeeeee
set number
set noexpandtab
set tabstop=2
set foldmethod=indent
set shiftwidth=2
set noeb vb t_vb=
set backspace=indent,eol,start
"set autoindent
"set smartindent
set mouse=a
set wrapscan
"hi link htmlLink NONE
set cursorline
"set cursorcolumn
hi cursorline cterm=none ctermbg=230
hi cursorcolumn cterm=none ctermbg=230
"hi cursor cterm=none ctermbg=15
"hi icursor cterm=none ctermbg=15
execute pathogen#infect()

